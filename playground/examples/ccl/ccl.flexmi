<?nsuri ccl?>
<_>
    <!-- Computes (NOT A) OR (B and C) -->
    <component name="Not">
        <in name="in"/>
        <out name="out"/>
    </component>
    <component name="And">
        <in name="a"/>
        <in name="b"/>
        <out name="out"/>
    </component>
    <component name="Or">
        <in name="a"/>
        <in name="b"/>
        <out name="out"/>
    </component>
    <connector from="Not.out" to="Or.a"/>
    <connector from="And.out" to="Or.b"/>
</_>